{
  "name": "ksw-rpom-icon-vue",
  "version": "0.1.0",
  "license": "ISC",
  "description": "KSW RPOM Icon",
  "main": "packages/cjs/index.js",
  "module": "packages/es/index.js",
  "files": [
    "styles",
    "icons.json",
    "packages/*/*.js",
    "packages/*/*/*.js",
    "packages/*/*.d.ts",
    "packages/*/*/*.d.ts"
  ],
  "scripts": {
    "compiler:icons": "rm -rf src/icons && node bin/build.mjs",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:code": "rollup -c",
    "lint": "vue-cli-service lint",
    "clean-publish": "clean-publish"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.16",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.4.20",
    "cheerio": "^1.0.0",
    "clean-publish": "^5.0.0",
    "core-js": "^3.8.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "gsap": "^3.12.5",
    "postcss": "^8.4.41",
    "rollup": "^4.20.0",
    "rollup-plugin-empty-dir": "^1.0.5",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.9",
    "vue": "^2.6.14",
    "vue-clipboard2": "^0.3.3",
    "vue-m-message": "3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "exports": {
    ".": {
      "import": "./packages/es/index.js",
      "require": "./packages/cjs/index.js"
    },
    "./icons.json": {
      "import": "./icons.json",
      "require": "./icons.json"
    },
    "./styles/*.css": {
      "import": "./styles/*.css",
      "require": "./styles/*.css"
    }
  },
  "clean-publish": {
    "fields": [
      "config",
      "author"
    ],
    "packageManager": "yarn"
  },
  "config": {
    "style": "fill",
    "size": "24"
  }
}
